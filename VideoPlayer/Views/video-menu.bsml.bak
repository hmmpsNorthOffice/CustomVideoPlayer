<bg id='root-object' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
  <bg id='video-details'>
    <horizontal pad='5'>
      <vertical child-control-width='false' horizontal-fit='PreferredSize' vertical-fit='PreferredSize'>
        <vertical id='current-video-player' pref-height='27' pref-width='48' pad-left='80'></vertical>
        <vertical pad-top='5' preferred-width='80'>
          <horizontal spacing='3' horizontal-fit='PreferredSize'>
            <button id='previous-video-button' hover-hint='Previous Video' text='Previous' font-align='Center' pad='0' on-click='on-previous-video-action' all-uppercase='false' pref-width='15' pref-height='5' font-size='3'></button>
            <text id='current-video-title' font-align='Center' font-size='3' overflow-mode='Ellipsis'></text>
            <button id='next-video-button' hover-hint='Next Video' text='Next' font-align='Center' pad='0' on-click='on-next-video-action' all-uppercase='false' pref-width='15' pref-height='5' font-size='3'></button>
          </horizontal>
          <vertical preferred-width='70' preferred-height='15'>
            <vertical align='left' pad-right='40'>
            <button id='video-source-priority-button'  text='Local First' hover-hint='Choose video source location priority' on-click='on-video-source-priority-action' preferred-height='5' font-size='3'></button>
            </vertical>
              <text id='general-info-message' word-wrapping='true' font-align='Left' overflow-mode='Ellipsis' font-size='3'></text>
          </vertical>

        </vertical>
      </vertical>
      <vertical pad-top='15' horizontal-fit='PreferredSize' spacing='3' pad-bottom='30'>
        <horizontal spacing='3' horizontal-fit='PreferredSize'>
          <text id='offset-text' text='Offset' align='Center' font-size='3'></text>
          <button id='offset-decrease-button' hover-hint='Starts Video Later' text='-' pad='0' on-click='on-offset-decrease-action' all-uppercase='false' pref-width='5' pref-height='5'></button>
          <text id='current-video-offset' text='0.0' align='Center'></text>
          <button id='offset-increase-button' hover-hint='Starts Video Earlier' text='+' pad='0' on-click='on-offset-increase-action' all-uppercase='false' pref-width='5' pref-height='5'></button>

        </horizontal>
        <horizontal>
        <button id='offset-magnitude-button' text='0.1 sec' on-click='on-offset-magnitude-action' preferred-height='5' pref-width='25' pad='0' font-size='3'></button>
        <button id='offset-reset-button' hover-hint='Resets offset to zero' text='Zero' pad='0' on-click='on-offset-reset-action' all-uppercase='false' font-size='3' pref-width='10' pref-height='5'></button>
      </horizontal>
        <button id='looping-button' text='Loop' on-click='on-looping-action' preferred-height='5' font-size='3'></button>
        <text pad-bottom='15' id='download-state-text' text='Downloaded' align='Center' font-size='3'></text>
         


        <horizontal spacing='3' horizontal-fit='PreferredSize'>
          <text id='speed-text' text='Speed' align='Center' font-size='3'></text>
          <button id='speed-decrease-button' hover-hint='Decrease Speed' text='-' pad='0' on-click='on-speed-decrease-action' all-uppercase='false' pref-width='5' pref-height='5'></button>
          <text id='current-video-speed' text='1'  align='Center'></text>
          <button id='speed-increase-button' hover-hint='Increase Speed' text='+' pad='0' on-click='on-speed-increase-action' all-uppercase='false' pref-width='5' pref-height='5'></button>
        </horizontal>

        <button id='speed-magnitude-button' text='+1' on-click='on-speed-magnitude-action' preferred-height='5' font-size='3'></button>
        
      </vertical>
    </horizontal>
        <bottom-button-panel pad-top='3' font-size='2'>
      <button id='rolling-video-queue-button' text='1 Vid' on-click='on-rolling-video-queue-action'/>
      <button id='rolling-offset-button' text='Fixed' on-click='on-enable-rolling-offset'/>     
      <button id='delete-button' text='Delete' on-click='on-delete-action'/>
      <button id='preview-button' text='Preview' on-click='on-preview-action'/>
      <button id='search-button' text='Search' on-click='on-search-action'/>
      <button id='enable-mvp-button' text='MVP Off' on-click='on-enable-mvp-action'/>
     </bottom-button-panel>
  </bg>
  <bg id='video-search-results'>
    <vertical pad-bottom='8' preferred-width='100'>
      <page-button click-event='video-list#PageUp' direction='Up' pref-width='30' pref-height='5'></page-button>
      <list id='video-list' list-width='100' visible-cells='4' select-cell='on-select-cell'></list>
      <page-button click-event='video-list#PageDown' direction='Down' pref-width='30' pref-height='5'></page-button>
    </vertical>
    <text id='search-results-loading' active='false' text='Loading Results...' align='Center' anchor-pos-y='5'/>
    <modal-keyboard id='search-keyboard' on-enter='on-query' clear-on-open='false' show-event='show-keyboard' hide-event='hide-keyboard' move-to-center='true' click-off-closes='true'></modal-keyboard>
    <bottom-button-panel pad-top='3'>
      <button text='Back' on-click='on-back-action'/>
      <button id='download-button' text='Download' on-click='on-download-action'/>
      <button id='refine-button' text='Refine' on-click='on-refine-action'/>
    </bottom-button-panel>
  </bg>
</bg>